
{
    "source": {
      "includePattern": ".+\\.js(doc|x)?$",
      "include": ["."],
      "exclude": ["node_modules", "downloads"]
    },
    "recurseDepth": 10,
    "plugins": ["plugins/markdown"],
    "opts": {
      "encoding": "utf8",
      "destination": "docs/",
      "template": "./node_modules/clean-jsdoc-theme",
      "recurse": true,
      "verbose": true,
      "readme": "../../README.md",
      "theme_opts": {
        "title": "EC Test Stack: Core Library"
      }
    }
  }